! Xcursor --------------------------------------------------------------------

Xcursor.theme: Vanilla-DMZ-AA
Xcursor.size:  22

! Xft settings ---------------------------------------------------------------

Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight

! xterm settings --------------------------------------------------------------
!UXTerm*faceName: Ubuntu Mono for Powerline
!UXTerm*faceName: Source Code Pro for Powerline
!UXTerm*faceName: Inconsolata for Powerline
UXTerm*faceName: DejaVu Sans Mono for Powerline
UXTerm*faceSize: 12

UXTerm*eightBitInput: false
UXTerm*metaSendsEscape: false

UXTerm*fontMenu*fontdefault*Label: Default
UXTerm*loginshell: 1
UXTerm*locale: 1
UXTerm*utf8: 1
UXTerm*utf8Title: 1

UXTerm*bellIsUrgent: 1
UXTerm*borderWidth: 0
UXTerm*scrollBar: 0

UXTerm*xftAntialias: 1
UXTerm*cjkWidth: 0

